<http://w3id.org/ocds/ns#> rdf:type owl:Ontology ;
                           
                           dcterms:title "Schema for an Open Contracting Release"@en ;
                           
                           dcterms:license <https://creativecommons.org/licenses/by/2.0/> ;
                           
