  rdfs:comment "Amount as a number."@en ;
  rdfs:range xsd:integer ;
  rdfs:domain :Value .

:currency a owl:FunctionalProperty ;
  rdfs:label "Currency"@en ;
  rdfs:comment "The currency in 3-letter ISO 4217 format."@en ;
  rdfs:range xsd:string ;
  rdfs:domain :Value .

